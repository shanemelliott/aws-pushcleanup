{
  "name": "aws-pinpoint-cleanup",
  "version": "1.0.0",
  "description": "Node.js application to clean up orphaned AWS Pinpoint push ARNs by checking their status and updating MSSQL database",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "cleanup": "node src/cleanup.js",
    "cleanup:staging": "cross-env ENVIRONMENT=staging node src/cleanup.js",
    "cleanup:production": "cross-env ENVIRONMENT=production node src/cleanup.js",
    "stats:staging": "cross-env ENVIRONMENT=staging node src/cleanup.js --stats",
    "stats:production": "cross-env ENVIRONMENT=production node src/cleanup.js --stats",
    "health-server": "node src/health-server.js",
    "deploy:setup": "node deploy.js setup",
    "deploy:auto": "node deploy.js start-auto",
    "deploy:status": "node deploy.js status",
    "deploy:logs": "node deploy.js logs",
    "deploy:monitor": "node deploy.js monitor"
  },
  "keywords": [
    "aws",
    "pinpoint",
    "cleanup",
    "mssql",
    "push-notifications"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-pinpoint": "^3.645.0",
    "@aws-sdk/client-sns": "^3.901.0",
    "@aws-sdk/client-sts": "^3.901.0",
    "dotenv": "^16.4.5",
    "mssql": "^11.0.1",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "nodemon": "^3.1.4",
    "pm2": "^5.3.0"
  }
}
